
def cislo_mensi_nez_3(hodnota):
   if hodnota > 3:
       print(f"hodnota {hodnota} je vetsi bez 3")
    elif hodnota < 3:
       print(f"hodnota {hodnota} je mensi bez 3")
    else:
       print(F"Hodnota {hodnota} je rovna 3")
       
    




if __name__ == "__main__":
    cislo = input ("Zadej cislo")
    cislo = int(cislo)
    cislo*= 3
    print(f"Zadane cislo je:" , cislo)5

    cislo_mensi_nez_3(1)
    cislo_mensi_nez_3(cislo)